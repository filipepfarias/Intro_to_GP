clear all, close all

% no =50;
X = linspace(0,1,no);
X = X';
Y = sin(2*pi*X);
e = 0.1.*randn(1,no);
e = e';
t = Y + e;
E = zeros(length(X));
% m = 50;
for m=0:
    mu = [0:m-1];
    mu = mu';
    for i=1:numel(mu)
        f(:,i) = X.^i;
    end
    w = (f'*f)\f'*t;
    y_hat = f*w;
    E(m) = sqrt(2*sum(y_hat - Y)^2/length(y_hat));
end

plot([],Y)
% plot(X,Y)
% hold on
% plot(X,t,'o')
% hold on
% plot(X,y_hat,'*')
% legend('Function','Data','Prediction')

%figure();
%plot(X,f)
